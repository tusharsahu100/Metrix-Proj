{
  "name": "ng-website-framework",
  "version": "1.0.0",
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "lite": "lite-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "ngc": "ngc -p tsconfig-aot.json && ngc -p public-tsconfig-aot.json",
    "rollup": "rollup -c rollup-config.js && rollup -c public-rollup-config.js",
    "build:aot": "ngc -p tsconfig-aot.json && ngc -p public-tsconfig-aot.json && rollup -c rollup-config.js && rollup -c public-rollup-config.js",
    "cleancss": "cd assets/styles  && cleancss --output sitestyle.min.css --keep-line-breaks --skip-advanced app.component.css spinner.component.css public.component.css toastr.component.css product-dialog.component.css ng2-select.css"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/animations": "4.0.0",
    "@angular/common": "4.0.0",
    "@angular/compiler": "4.0.0",
    "@angular/core": "4.0.0",
    "@angular/forms": "4.0.0",
    "@angular/http": "4.0.0",
    "@angular/platform-browser": "4.0.0",
    "@angular/platform-browser-dynamic": "4.0.0",
    "@angular/router": "4.0.0",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-beta.5",
    "angular2-logger": "0.6.0",
    "bootstrap": "3.3.7",
    "core-js": "2.4.1",
    "intl": "1.2.5",
    "moment": "2.18.1",
    "mydatepicker": "2.0.31",
    "ng2-bs3-modal": "0.10.4",
    "ng2-cookies": "1.0.12",
    "ng2-select": "1.2.0",
    "ng2-toastr": "1.4.1",
    "ng2-translate": "5.0.0",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.4.1",
    "systemjs": "0.20.19",
    "zone.js": "0.8.14"
  },
  "devDependencies": {
    "@angular/compiler-cli": "4.0.0",
    "@angular/platform-server": "4.0.0",
    "@types/core-js": "0.9.35",
    "@types/node": "6.0.60",
    "clean-css": "3.4.23",
    "concurrently": "3.5.0",
    "lite-server": "2.3.0",
    "rollup": "0.50.0",
    "rollup-plugin-commonjs": "8.2.1",
    "rollup-plugin-includepaths": "0.2.2",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-uglify": "2.0.1",
    "typescript": "2.3.3"
  }
